name \"secrets.NAME\"\n      - run: git add .\n      - run: git commit -m 'UP'\n      - run: git push\n\n$ vim book.json\n{\n    \"title\": \"个人笔记\",\n    \"description\": \"个人笔记\",\n    \"author\": \"Jett\",\n    \"language\": \"zh-hans\",\n    \"root\": \".\",\n\n    \"plugins\": [\n        \"github-buttons@2.1.0\",\n        \"edit-link\",\n    \"toggle-chapters\"\n    ],\n\n    \"pluginsConfig\": {\n        \"github-buttons\": {\n            \"repo\": \"huxiaoning/memory\",\n            \"types\": [\n                \"star\"\n            ],\n            \"size\": \"small\"\n        },\n        \"edit-link\": {\n            \"base\": \"https://github.com/huxiaoning/memory/edit/master\",\n            \"label\": \"Edit This Page\"\n        }\n    }\n}\n\n"},"01笔记系统/01Git.html":{"url":"01笔记系统/01Git.html","title":"Git","keywords":"","body":"Git\nGit\n"},"01笔记系统/02Typora.html":{"url":"01笔记系统/02Typora.html","title":"Typora","keywords":"","body":"Typora\n"}}}